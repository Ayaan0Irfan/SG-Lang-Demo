# API Keys - Copy this file to .env and add your real API keys

# Groq API Key (primary) - Get from https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API Key (optional fallback) - Get from https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face Token (optional, for some models) - Get from https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=your_hf_token_here

# Optional: Model settings
DEFAULT_MODEL=mixtral-8x7b-32768
EMBEDDING_MODEL=all-MiniLM-L6-v2
MAX_TOKENS=500
TEMPERATURE=0.3
